{
	"name": "it-portfolio-manager-frontend",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"build": "tsc && vite build",
		"dev": "vite",
		"fmt": "prettier --write src",
		"fmt.check": "prettier --check src",
		"precommit": "lint-staged --shell",
		"prepare": "husky install",
		"preview": "npx serve ./dist",
		"test": "vitest run",
		"test:watch": "vitest",
		"test.ui": "vitest --ui components",
		"coverage": "vitest --coverage",
		"qwik": "qwik"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^6.4.3",
		"@vitest/coverage-v8": "^1.5.3",
		"@vitest/ui": "^0.34.6",
		"autoprefixer": "^10.4.17",
		"husky": "^8.0.3",
		"jsdom": "^24.0.0",
		"lint-staged": "^15.2.0",
		"postcss": "8.4.33",
		"prettier": "^3.2.4",
		"tailwindcss": "3.4.1",
		"typescript": "^5.3.3",
		"vite": "^5.0.12",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^1.5.3"
	},
	"dependencies": {
		"@auth0/auth0-spa-js": "^2.1.3",
		"@builder.io/qwik": "^1.4.0",
		"ccat-api": "^0.10.6",
		"chart.js": "^4.4.1",
		"class-variance-authority": "^0.7.0",
		"date-fns": "^3.6.0",
		"flowbite": "^2.3.0"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,css,json}": "prettier --write"
	},
	"packageManager": "pnpm@8.15.5"
}
